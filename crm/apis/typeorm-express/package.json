{
  "name": "typeorm-express",
  "version": "0.0.0",
  "description": "Test to express latency",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev src/index.ts --swc",
    "build": "node build.js",
    "test": "NODE_ENV=test jest",
    "clean": "rm -r dist",
		"migration:generate": "npx typeorm-ts-node-esm migration:generate migrations/$NAME -d ./src/database.ts",
		"migration:create": "npx typeorm-ts-node-esm migration:create migrations/$NAME -d ./src/database.ts",
		"migration:run": "npx typeorm-ts-node-esm migration:run -d ./src/database.ts",
		"migration:revert": "npx typeorm-ts-node-esm migration:revert -d ./src/database.ts"
  },
  "author": "amoles",
  "license": "ISC",
  "dependencies": {
    "@latency/express-server": "*",
    "diod": "2.0.0",
    "firebase-admin": "^11.3.0",
    "jsonwebtoken": "8.5.1",
    "pg": "8.8.0",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.3.10",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/uuid": "8.3.4",
    "esbuild": "^0.14.54",
    "esbuild-jest": "0.5.0",
    "esbuild-plugin-decorator": "^0.4.0",
    "eslint-config-custom": "*",
    "jest": "29.3.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig": "*"
  }
}
